[Unit]
Description=AlertManager
Wants=network-online.target
After=network-online.target

[Service]
User=prometheus
Group=prometheus
Type=simple
ExecStart= /home/normaluser/alertmanager-0.25.0.linux-amd64/alertmanager \
--config.file=/home/normaluser/alertmanager-0.25.0.linux-amd64/alertmanager.yml

[Install]
WantedBy=multi-user.target
